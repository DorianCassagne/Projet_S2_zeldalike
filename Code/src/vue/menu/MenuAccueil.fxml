<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controler.menu.Accueil1Controler">
   <children>
      <VBox alignment="CENTER" prefHeight="640.0" prefWidth="960.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
            <VBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#newGame" prefHeight="30.0" prefWidth="200.0" text="Nouvelle Partie">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#loadMenuGame" prefHeight="30.0" prefWidth="200.0" text="Charger une partie" />
                  <Button mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="200.0" text="Quitter" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
